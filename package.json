{
  "name": "eos-icons-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.16.0"
  },
  "scripts": {
    "preinstall": "npm update eos-icons ",
    "postinstall": "grunt -b ./src moveFiles && cp node_modules/eos-icons/animated-svg/*.svg ./src/svg/",
    "build": "tsc --project ./",
    "start:dev": "grunt -b ./src moveFiles && NODE_PATH=./src nodemon ./src/server.ts",
    "start": "grunt -b ./src moveFiles && NODE_PATH=./src ts-node --transpile-only ./src/server.ts",
    "updateDBicons": "NODE_PATH=./src ts-node --transpile-only ./src/utils/updateDBIcons.ts",
    "lint": "eslint \"src/*\"",
    "lint:fix": "eslint \"src/*\" --fix",
    "test:v1": "grunt -b ./src moveFiles && NODE_ENV=test NODE_PATH=./src ts-node --transpile-only ./src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EOS-uiux-Solutions/eos-icons-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EOS-uiux-Solutions/eos-icons-api/issues"
  },
  "homepage": "https://github.com/EOS-uiux-Solutions/eos-icons-api#readme",
  "devDependencies": {
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.1",
    "@types/pify": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "eos-icons": "^5.1.0",
    "express": "^4.17.1",
    "grunt": "^1.4.1",
    "grunt-cli": "^1.4.3",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-sass": "^3.1.0",
    "grunt-text-replace": "^0.4.0",
    "grunt-webfont": "^1.7.2",
    "mongoose": "^5.12.13",
    "newman": "^5.2.3",
    "node-cmd": "^4.0.0",
    "pify": "^3.0.0",
    "svg2img": "^0.9.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "winston": "^3.3.3",
    "zip-folder": "^1.0.0"
  }
}
